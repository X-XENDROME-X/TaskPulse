{
    "manifest_version": 3,
    "name": "TaskPulse",
    "version": "1.0",
    "description": "A Chrome extension for managing your to-do tasks.",
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "permissions": ["storage"]
  }
  